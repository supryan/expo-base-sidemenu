{
    "expo": {
        "name": "Expo Base (Side Menu)",
        "slug": "expo-base-sidemenu",
        "privacy": "public",
        "sdkVersion": "37.0.0",
        "platforms": [
            "ios",
            "android"
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "packagerOpts": {
            "config": "metro.config.js",
            "sourceExts": [
                "js",
                "jsx",
                "svg",
                "ts",
                "tsx"
            ]
        },
        "assetBundlePatterns": [
            "**/*"
        ]
    }
}
